// Test Sparse Conditional Constant Propagation
// See Figure 12.34 of Whale Book

fn @main() {
%B1:
    $a.1 <- mov i64 3;
    $d.1 <- mov i64 2;
    jmp %B2;
%B2:
    $d.3 <- phi i64 [%B1: $d.1] [%B5: $d.2];
    $a.3 <- phi i64 [%B1: $a.1] [%B5: $a.2];
    $f.1 <- add i64 $a.3, $d.3;
    $g.1 <- mov i64 5;
    $a.2 <- sub i64 $g.1, $d.3;
    $c.1 <- le i64 $f.1, $g.1;
    br $c.1 ? %B3 : %B4;
%B3:
    $f.2 <- add i64 $g.1, 1;
    jmp %B5;
%B4:
    $e.1 <- lt i64 $g.1, $a.2;
    br $e.1 ? %B5 : %B6;
%B5:
    $d.2 <- mov i64 2;
    jmp %B2;
%B6:
    ret;
}