@g <- 0: i64;

fn @max(%a: i64, %b: i64) -> i64 {
%Begin:
    %c <- ge i64 %a, %b;
    br %c ? %True : %False;
%True:
    %x.0 <- @a;
    jmp %End;
%False:
    %x.1 <- @b;
    jmp %End;
%End:
    %x.2 <- phi i64 [%True: %x.0] [%False: %x.1];
    ret %x.2;
}

fn @main() {
%Begin:
    @g <- call i64 @max(1, 2);
    ret;
}